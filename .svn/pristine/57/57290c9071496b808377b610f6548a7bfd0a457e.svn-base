

public class Act {
	
	private String type;
	private int x;
	private int y;
	private int unit;
	
	public Act(String _type,int _unit,int _x,int _y) {
		type = _type;
		unit = _unit;
		x = _x;
		y = _y;
	}
	
	public String getType() {
		return type;
	}
	
	public int getX() {
		return x;
	}
	
	public int getY() {
		return y;
	}
	
	public int getUnit() {
		return unit;
	}
	
}