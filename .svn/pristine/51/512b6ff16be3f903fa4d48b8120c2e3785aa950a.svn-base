import java.io.*;
import java.net.*;
import java.util.*;

class Unit {
	
	private int x;
	private int y;
	private int owner;
	private int health;
	private int cooldown;
	private String type;
	private int id;
	
	public Unit() {
		
	}
	
	public int getId() {
		return id;
	}
	
	public int getX() {
		return x;
	}
	
	public int getY() {
		return y;
	}
	
	public int getHealth() {
		return health;
	}
	
	public int getCooldown() {
		return cooldown;
	}
	
	public int getOwner() {
		return owner;
	}
	
	public String getType() {
		return type;
	}
	
}